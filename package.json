{
  "name": "ogma",
  "version": "0.0.0",
  "private": true,
  "description": "The @ogma workspace containing the base logging package, the NestJS module, and all the fun parsers to add in.",
  "author": "Jay McDoniel <me@jaymcdoniel.dev>",
  "contributors": [
    "Minsung Kim",
    "John Biundo",
    "Ruslan Gonzalez"
  ],
  "keywords": [
    "nestjs",
    "nest",
    "logger",
    "json",
    "json-logging",
    "request-logging",
    "nestjs-module",
    "module",
    "logging",
    "microservice",
    "http",
    "gql",
    "graphql",
    "websocket",
    "ws"
